<script type="text/javascript">

(function() {
    var wrapper = document.getElementById("ie-warning-wrapper");
    var closeLink = document.getElementById("ie-warning-wrapper-close");
    var cookieName = "ieWarningBannerSnooze";

    var setCookie = function() {
        var date = new Date();
        date.setDate(date.getDate() + 1);
        document.cookie = (
            cookieName + "=1; expires=" + date.toUTCString() + "; path=/"
        );
    };

    var hasCookie = function() {
        return document.cookie.indexOf(cookieName + "=1") !== -1;
    };

    if (wrapper) {
        if (!hasCookie()) {
            wrapper.style.display = "block";
        }
    }

    if (closeLink) {
        closeLink.onclick = function() {
            setCookie();
            if (wrapper) {
                wrapper.style.display = "none";
            }
        }
    }
})();

</script>
